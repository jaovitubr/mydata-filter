{
  "name": "odata-filter",
  "description": "This library makes it easy to filter data queries in the remaining API",
  "version": "1.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run generate && npm run clean && tsc",
    "generate": "nearleyc ./src/rules.ne -o ./src/rules.ts",
    "prepare": "npm run build"
  },
  "author": "Jo√£o Vitor Machado",
  "license": "MIT",
  "homepage": "https://github.com/joaovitmac/odata-filter#readme",
  "bugs": {
    "url": "https://github.com/joaovitmac/odata-filter/issues"
  },
  "keywords": [
    "odata",
    "querystring",
    "parser",
    "filter",
    "rest"
  ],
  "devDependencies": {
    "@types/moo": "^0.5.5",
    "@types/nearley": "^2.11.2",
    "@types/node": "^16.3.1",
    "moo": "^0.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "nearley": "^2.20.1"
  }
}
